      "inScheme": "https://ictconnect21.jp/xapi/profiles/mexcbt/v1",
      "prefLabel": {
        "ja": "採点形式",
        "en": "Scoring Type"
      },
      "definition": {
        "ja": "採点の形式（例：'auto'）を示します。",
        "en": "Indicates the type of scoring (e.g., 'auto')."
      }
    }
